<%= provide(:title, 'Edit place') %>
<h1>Editing Place</h1>

<div class="row">
  <div class="span8 offset2">
    <%= form_for(@place) do |f| %>
      <%= render 'shared/error_messages', object: @place %>
      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.submit 'Save changes', class: 'btn btn-large btn-primary' %>
    <% end %>
  </div>
</div>